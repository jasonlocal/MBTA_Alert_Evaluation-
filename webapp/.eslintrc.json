{
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "extends": [
        "airbnb",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 2],
        "no-console": 0,
        "react/prefer-stateless-function": 0,
        "react/prop-types": 0,
        "import/prefer-default-export": 0
    }
}
